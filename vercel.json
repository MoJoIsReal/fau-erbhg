{
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/login",
      "destination": "/api/login.js"
    },
    {
      "source": "/api/user", 
      "destination": "/api/user.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/[...route].js"
    }
  ]
}